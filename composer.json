{
    "name": "aodihis/get-video-id",
    "description": "Twig filter to get the video id of youtube or Vimeo URL.",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "get video id"
    ],
    "support": {
        "docs": "https://github.com/aodihis/craftcms-get-video-id#readme"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "aodihis"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
          "aodihis\\getvideoid\\": "src/"
        }
    },
    "extra": {
        "name": "Get Video Id",
        "handle": "get-video-id",
        "developer": "aodihis",
        "documentationUrl": "https://github.com/aodihis/craftcms-get-video-id#readme",
